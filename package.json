{
  "private": true,
  "name": "wheresmyvaccine.org",
  "homepage": "https://wheresmyvaccine.org",
  "description": "Help people determine Vaccine Eligibility",
  "keywords": [ "vaccine", "health", "eligibility", "vax", "disease" ],
  "version": "0.0.0",
  "license": "ISC",
  "main": "index.html",
  "scripts": {
    "start": "npm run dev",
    "build": "preact build && cp ./CNAME build/",
    "serve": "bundle exec jekyll serve -s build",
    "deploy": "node ./scripts/deploy.js",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [ "build/", "_site" ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "gh-pages": "^3.1.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "preact-cli": "^3.0.0",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [ "<rootDir>/tests/__mocks__/browserMocks.js", "<rootDir>/tests/__mocks__/setupTests.js" ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheresmyvaccine/wheresmyvaccine.github.io.git"
  }
}
